{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@webgpu/types",
      "./node_modules/@types",
      "../../node_modules/@types",
      "../../node_modules/@webgpu/types"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext"
  }
}
